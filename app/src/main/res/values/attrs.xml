<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>



    <declare-styleable name="core_area">
        <attr name="underline_color"
            format="color"/>
        <attr name="underline_selected_color"
            format="color"/>
        <attr name="hint_color"
            format="color"/>
        <attr name="text_color"
            format="color"/>
        <attr name="hint_text"
            format="string"/>
        <attr name="codes"
            format="integer"/>
    </declare-styleable>

    <declare-styleable name="TextAlphaSquareTextView">
        <attr name="square_mode" format="boolean"/>
    </declare-styleable>

</resources>